<div class="container">

	<h1>Import Jobs</h1>

	<%= form_for @conn, Routes.job_path(@conn, :save_import), [multipart: true], fn f-> %>

	<%= label f, :upload, "CSV File (RFC4180)" %>
	<%= file_input f, :upload %>

	<div class="actions">
		<%= link "Back", to: Routes.job_path(@conn, :index), class: 'button button-outline' %>
		<%= submit "Import" %>
	</div>
	<% end %>

</div>
